<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Title>Salamander Blazor</Title>
    <Company>Jon Gucciardi</Company>
    <Description>testing Razor Library </Description>
    <PackageProjectUrl>https://github.com/SalamanderVertigo/SalamanderBlazor</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SalamanderVertigo/SalamanderBlazor</RepositoryUrl>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.8" />
    <PackageReference Include="MudBlazor" Version="6.0.14" />
  </ItemGroup>

</Project>
